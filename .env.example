# Default reasoning mode: openai / bedrock
OE_REASONING_MODE=openai

# OpenAI Provider (used when OE_REASONING_MODE=openai)
OE_OPENAI_API_KEY=
OE_OPENAI_MODEL=gpt-4o-mini

# Bedrock Provider (used when OE_REASONING_MODE=bedrock)
OE_AWS_REGION=us-east-1
OE_BEDROCK_MODEL=amazon.titan-text-lite-v1

# OPTIONAL for local dev (NOT recommended inside Lambda)
OE_AWS_ACCESS_KEY_ID=
OE_AWS_SECRET_ACCESS_KEY=

USE_BEDROCK=false